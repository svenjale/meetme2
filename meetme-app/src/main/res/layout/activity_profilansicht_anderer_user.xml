<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/schlossblurr"
    tools:context="de.meetme.profilansicht"
   >

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:text="Profil"
        android:textColor="#000000"
        android:textSize="22dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="300dp"
        android:layout_height="31dp"
        android:layout_marginTop="9dp"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="247dp"
        android:textColor="#000000"
        android:layout_below="@+id/textView36"
        android:layout_toRightOf="@+id/imageView4"
        android:layout_toEndOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="63dp"
        android:text="Vorname:"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="137dp"
        android:layout_below="@+id/button14"
        android:layout_toRightOf="@+id/imageView5"
        android:layout_toEndOf="@+id/imageView5" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView28"
        android:layout_alignBottom="@+id/textView28"
        android:layout_alignLeft="@+id/textView37"
        android:layout_alignStart="@+id/textView37"
        android:text="Nachname"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="216dp" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="100dp"
        android:layout_height="31dp"
        android:layout_marginTop="7dp"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="166dp"
        android:layout_below="@+id/textView28"
        android:layout_toRightOf="@+id/imageView3"
        android:layout_toEndOf="@+id/imageView3" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Rolle:"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="299dp"
        android:layout_below="@+id/textView37"
        android:layout_alignLeft="@+id/textView39"
        android:layout_alignStart="@+id/textView39"
        android:layout_marginTop="11dp" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Telefonnummer:"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="383dp"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_toRightOf="@+id/imageView4"
        android:layout_toEndOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="100dp"
        android:layout_height="31dp"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="413dp"
        android:layout_alignBaseline="@+id/textView33"
        android:layout_alignBottom="@+id/textView33"
        android:layout_toRightOf="@+id/textView36"
        android:layout_toEndOf="@+id/textView36" />

    <Button
        android:id="@+id/button2"
        android:layout_width="94dp"
        android:layout_height="58dp"
        android:background="#21fffafa"
        android:text="Profil"
        android:textSize="13dp"
        android:drawableTop="@drawable/ic_person_black_24dp"
        android:paddingTop="7dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/checkBox"
        android:layout_alignEnd="@+id/checkBox" />

    <Button
        android:id="@+id/button3"
        android:layout_width="92dp"
        android:layout_height="58dp"
        android:background="#21fffafa"
        android:text="Map"
        android:textSize="13dp"
        android:drawableTop="@drawable/ic_map_black_24dp"
        android:paddingTop="7dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button2"
        android:layout_toRightOf="@+id/button2" />

    <Button
        android:id="@+id/button6"
        android:layout_width="96dp"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:textSize="13dp"
        android:drawableTop="@drawable/ic_people_black_24dp"
        android:paddingTop="7dp"
        android:background="#21fffafa"
        android:text="Kontakte"
        />

    <Button
        android:id="@+id/button7"
        android:layout_width="92dp"
        android:layout_height="58dp"
        android:background="#21fffafa"
        android:text="Walks"
        android:textSize="13dp"
        android:drawableTop="@drawable/ic_directions_walk_black_24dp"
        android:paddingTop="7dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/button3"
        android:layout_toLeftOf="@+id/button6"
        android:layout_toStartOf="@+id/button6" />

    <Button
        android:id="@+id/button15"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:drawableLeft="@drawable/ic_star_black"
        android:paddingLeft="5dp"
        android:text="ZU KONTAKTEN"
        android:background="@drawable/mybutton"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/textView34"
        android:layout_alignLeft="@+id/imageView4"
        android:layout_alignStart="@+id/imageView4"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="12dp" />

    <Button
        android:id="@+id/button16"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:drawableLeft="@drawable/ic_cancel_black_24dp"
        android:paddingLeft="5dp"
        android:text="ENTFERNEN"
        android:background="@drawable/mybutton"
        android:layout_alignTop="@+id/button15"
        android:layout_toRightOf="@+id/button3"
        android:layout_toEndOf="@+id/button3" />


    <TextView
        android:id="@+id/textView39"
        android:layout_width="300dp"
        android:layout_height="31dp"
        android:layout_marginTop="11dp"
        android:textColor="#000000"
        android:layout_below="@+id/textView35"
        android:layout_toRightOf="@+id/imageView4"
        android:layout_toEndOf="@+id/imageView4" />

    <ImageButton
        android:id="@+id/button18"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/mybutton"
        android:text="Nachricht senden"
        android:src="@drawable/handymsg"
        android:layout_below="@+id/textView39"
        android:layout_toRightOf="@+id/button7"
        android:layout_toEndOf="@+id/button7" />

    <ImageButton
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/phoneicon"
        android:background="@drawable/mybutton"
        android:id="@+id/imageButton3"
        android:layout_below="@+id/textView39"
        android:layout_alignLeft="@+id/button14"
        android:layout_alignStart="@+id/button14" />

    <Button
        android:id="@+id/button19"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:background="@drawable/mybutton"
        android:text="Photowalk teilnahmen"
        android:layout_marginTop="33dp"
        android:layout_below="@+id/button15"
        android:layout_centerHorizontal="true" />
    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/roleicon"
        android:layout_marginLeft="5dp"
        android:layout_above="@+id/textView39"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/handy"
        android:layout_marginLeft="5dp"
        android:layout_below="@+id/textView39"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/faceicon"
        android:layout_marginLeft="5dp"
        android:layout_alignBottom="@+id/textView28"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/button14"
        android:layout_width="40dp"
        android:src="@drawable/helpicon"
        android:layout_height="40dp"
        android:background="@drawable/mybutton"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/button19"
        android:layout_toEndOf="@+id/button19"
        android:layout_marginTop="8dp" />


</android.widget.RelativeLayout>


<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/EigenesProfilbild"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/schlossblurr"
    tools:context="de.meetme.ProfilAnsichtEigenesProfil">

    <TextView
        android:id="@+id/textView34"
        android:layout_width="300dp"
        android:layout_height="31dp"
        android:layout_below="@+id/imageView4"
        android:layout_toEndOf="@+id/imageView4"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="247dp" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button22"
        android:layout_marginTop="58dp"
        android:layout_toEndOf="@+id/imageView5"
        android:text="Name:"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="137dp" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_above="@+id/textView37"
        android:layout_toEndOf="@+id/imageView3"
        android:text="Rolle:"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="299dp" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignBottom="@+id/imageView4"
        android:layout_toEndOf="@+id/imageView4"
        android:text="Telefonnummer:"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="383dp" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="100dp"
        android:layout_height="31dp"
        android:layout_below="@+id/imageView3"
        android:layout_toEndOf="@+id/imageView3"
        android:layout_toStartOf="@+id/button7"
        tools:layout_editor_absoluteX="14dp"
        tools:layout_editor_absoluteY="413dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="94dp"
        android:layout_height="58dp"
        android:layout_alignEnd="@+id/checkBox"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/checkBox"
        android:background="#21fffafa"
        android:drawableTop="@drawable/ic_person_black_24dp"
        android:paddingTop="7dp"
        android:text="Profil"
        android:textSize="13dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="92dp"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button2"
        android:layout_toRightOf="@+id/button2"
        android:background="#21fffafa"
        android:drawableTop="@drawable/ic_map_black_24dp"
        android:paddingTop="7dp"
        android:text="Map"
        android:textSize="13dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="96dp"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="#21fffafa"
        android:drawableTop="@drawable/ic_people_black_24dp"
        android:paddingTop="7dp"
        android:text="Kontakte"
        android:textSize="13dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="92dp"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/button6"
        android:layout_toRightOf="@+id/button3"
        android:layout_toStartOf="@+id/button6"
        android:background="#21fffafa"
        android:drawableTop="@drawable/ic_directions_walk_black_24dp"
        android:paddingTop="7dp"
        android:text="Walks"
        android:textSize="13dp" />

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_below="@+id/textView34"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="41dp"
        android:background="@drawable/mybutton"
        android:text="Bearbeiten" />

    <ImageButton
        android:id="@+id/button14"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignTop="@+id/button22"
        android:layout_toEndOf="@+id/button16"
        android:layout_toRightOf="@+id/button16"
        android:background="@drawable/mybutton"
        android:src="@drawable/helpicon" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="300dp"
        android:layout_height="31dp"
        android:layout_below="@+id/imageView5"
        android:layout_toEndOf="@+id/imageView5"
        android:layout_toStartOf="@+id/EigenesProfilBild" />

    <Button
        android:id="@+id/button20"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignStart="@+id/button16"
        android:layout_below="@+id/button16"
        android:layout_marginTop="15dp"
        android:background="@drawable/mybutton"
        android:text="Photowalk teilnahmen" />

    <ImageButton
        android:id="@+id/button22"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="8dp"
        android:layout_toLeftOf="@+id/button16"
        android:layout_toStartOf="@+id/button16"
        android:background="@drawable/mybutton"
        android:src="@drawable/logouticon"
        android:text="logout" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView28"
        android:layout_marginTop="36dp"
        android:background="@drawable/roleicon"
        android:paddingLeft="5dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView37"
        android:background="@drawable/phoneicon"
        android:paddingLeft="5dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView28"
        android:background="@drawable/faceicon"
        android:paddingLeft="5dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:text="Dein Profil"
        android:textColor="#000000"
        android:textSize="22dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/EigenesProfilBild"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignBottom="@+id/textView37"
        android:layout_alignTop="@+id/imageZoom"
        android:layout_toEndOf="@+id/textView36"
        android:layout_toStartOf="@+id/button14"></ImageView>

    <ImageButton
        android:id="@+id/imageZoom"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_alignBottom="@+id/textView35"
        android:layout_alignEnd="@+id/textView39"
        android:layout_below="@+id/button14"
        android:layout_marginTop="20dp"
        android:layout_toEndOf="@+id/textView37"
        android:background="@null" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/photo_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


</android.widget.RelativeLayout>

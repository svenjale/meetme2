<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/schlossblurr"
    tools:context="de.meetme.eventinfos">

    <Button
        android:id="@+id/button26"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:background="@drawable/mybutton"
        android:text="Von Event abmelden"
        android:layout_marginTop="14dp"
        android:layout_below="@+id/button5"
        android:layout_alignLeft="@+id/button11"
        android:layout_alignStart="@+id/button11" />

    <ImageButton
        android:id="@+id/button14"
        android:layout_width="40dp"
        android:src="@drawable/helpicon"
        android:layout_height="40dp"
        android:background="@drawable/mybutton"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toRightOf="@+id/textView31"
        android:layout_toEndOf="@+id/textView31" />

    <ImageButton
        android:id="@+id/button21"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/shareicon"
        android:background="@drawable/mybutton"
        android:layout_alignTop="@+id/button23"
        android:layout_alignLeft="@+id/button14"
        android:layout_alignStart="@+id/button14" />

    <ImageButton
        android:id="@+id/button23"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/alarmicon"
        android:background="@drawable/mybutton"
        android:layout_marginTop="13dp"
        android:layout_alignTop="@+id/textView30"
        android:layout_toLeftOf="@+id/button21"
        android:layout_toStartOf="@+id/button21"
        android:layout_marginRight="15dp"
        android:layout_marginEnd="15dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:background="@drawable/mybutton"
        android:text="Teilnehmer"
        android:layout_below="@+id/textView17"
        android:layout_alignLeft="@+id/textView17"
        android:layout_alignStart="@+id/textView17"
        android:layout_marginLeft="10dp"
        android:layout_marginStart="10dp" />

    <ImageButton
        android:id="@+id/button12"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_above="@+id/button23"
        android:src="@drawable/calendar"
        android:scaleType="fitCenter"
        android:padding="10dp"
        android:background="@drawable/mybutton"
        android:layout_alignRight="@+id/button21"
        android:layout_alignEnd="@+id/button21" />


    <Button
        android:id="@+id/button9"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:background="@drawable/mybutton"
        android:drawableLeft="@drawable/personadd"
        android:drawableStart="@drawable/personadd"
        android:layout_below="@+id/textView17"
        android:text="TEILNEHMEN"
        android:paddingLeft="5dp"
        android:layout_alignLeft="@+id/button5"
        android:layout_alignStart="@+id/button5" />

    <Button
        android:id="@+id/button2"
        android:layout_width="94dp"
        android:layout_height="58dp"
        android:background="#21fffafa"
        android:text="Profil"
        android:textSize="13dp"
        android:drawableTop="@drawable/ic_person_black_24dp"
        android:paddingTop="7dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/checkBox"
        android:layout_alignEnd="@+id/checkBox" />

    <Button
        android:id="@+id/button3"
        android:layout_width="92dp"
        android:layout_height="58dp"
        android:background="#21fffafa"
        android:text="Map"
        android:textSize="13dp"
        android:drawableTop="@drawable/ic_map_black_24dp"
        android:paddingTop="7dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button2"
        android:layout_toRightOf="@+id/button2" />

    <Button
        android:id="@+id/button6"
        android:layout_width="96dp"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:textSize="13dp"
        android:drawableTop="@drawable/ic_people_black_24dp"
        android:paddingTop="7dp"
        android:background="#21fffafa"
        android:text="Kontakte"
        />

    <Button
        android:id="@+id/button7"
        android:layout_width="92dp"
        android:layout_height="58dp"
        android:background="#21fffafa"
        android:text="Walks"
        android:textSize="13dp"
        android:drawableTop="@drawable/ic_directions_walk_black_24dp"
        android:paddingTop="7dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/button3"
        android:layout_toLeftOf="@+id/button6"
        android:layout_toStartOf="@+id/button6" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="150dp"
        android:layout_height="30dp"
        android:textSize="22dp"
        android:layout_marginTop="15dp"
        android:text="Eventname"
        android:gravity="center"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView23"
        android:layout_alignRight="@+id/button23"
        android:layout_alignEnd="@+id/button23" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Beschreibung:"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Wo?"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        android:layout_below="@+id/textView16"
        android:layout_alignLeft="@+id/textView20"
        android:layout_alignStart="@+id/textView20" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:layout_below="@+id/textView23"
        android:layout_alignLeft="@+id/textView23"
        android:layout_alignStart="@+id/textView23" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="11dp"
        android:text="Wann?"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        android:layout_below="@+id/textView24"
        android:layout_alignLeft="@+id/textView23"
        android:layout_alignStart="@+id/textView23" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:text="Organisator:"
        android:textAppearance="@android:style/TextAppearance.Holo"
        android:textSize="20dp"
        android:layout_below="@+id/textView19"
        android:layout_alignLeft="@+id/textView19"
        android:layout_alignStart="@+id/textView19"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="300dp"
        android:textColor="#000000"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView20"
        android:layout_alignLeft="@+id/textView16"
        android:layout_alignStart="@+id/textView16" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="300dp"
        android:textColor="#000000"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_below="@+id/textView31"
        android:layout_alignLeft="@+id/textView23"
        android:layout_alignStart="@+id/textView23" />

    <Button
        android:id="@+id/button5"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:drawableStart="@drawable/personadd"
        android:drawableLeft="@drawable/personadd"
        android:background="@drawable/mybutton"
        android:text="ANWESEND"
        android:paddingLeft="5dp"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        android:layout_below="@+id/button9"
        android:layout_toRightOf="@+id/button10"
        android:layout_marginTop="9dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:background="@drawable/mybutton"
        android:text="Anwesende"
        android:layout_alignTop="@+id/button5"
        android:layout_alignLeft="@+id/button26"
        android:layout_alignStart="@+id/button26" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:layout_alignLeft="@+id/textView25"
        android:layout_alignStart="@+id/textView25"
        android:layout_below="@+id/textView30"
        android:textColor="#000000"
        android:layout_toStartOf="@+id/button24" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="300dp"
        android:textColor="#000000"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView25"
        android:layout_alignLeft="@+id/textView24"
        android:layout_alignStart="@+id/textView24" />

    <ImageButton
        android:id="@+id/button24"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/mybutton"
        android:src="@drawable/mapicon"
        android:layout_marginRight="13dp"
        android:layout_marginEnd="13dp"
        android:layout_alignTop="@+id/button23"
        android:layout_toLeftOf="@+id/button23"
        android:layout_toStartOf="@+id/button23" />

</android.widget.RelativeLayout>

<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentLeft="true"
    tools:context="de.meetme.teilnehmerlist">

    <Button
        android:id="@+id/button2"
        android:layout_width="94dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#4A1D"
        android:text="Profil" />

    <Button
        android:id="@+id/button3"
        android:layout_width="92dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/textView5"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/textView5"
        android:layout_toRightOf="@+id/button2"
        android:background="#4A1D"
        android:text="Map" />

    <Button
        android:id="@+id/button6"
        android:layout_width="96dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:background="#4A1D"
        android:text="Kontakte" />

    <Button
        android:id="@+id/button7"
        android:layout_width="92dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/button6"
        android:layout_toRightOf="@+id/button3"
        android:layout_toStartOf="@+id/button6"
        android:background="#4A1D"
        android:text="Walks" />


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:text="Teilnehmer"
        android:textSize="22dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="15dp"
        android:layout_toStartOf="@+id/button6" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView46"
        android:layout_alignLeft="@+id/textView46"
        android:layout_alignRight="@+id/textView46"
        android:layout_alignStart="@+id/textView46"
        android:layout_below="@+id/textView46"
        android:layout_marginTop="15dp" />

    <TextView
        android:id="@+id/textView44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView43"
        android:layout_alignLeft="@+id/textView43"
        android:layout_alignRight="@+id/textView43"
        android:layout_alignStart="@+id/textView43"
        android:layout_below="@+id/textView43"
        android:layout_marginTop="16dp" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView44"
        android:layout_alignLeft="@+id/textView44"
        android:layout_alignRight="@+id/textView44"
        android:layout_alignStart="@+id/textView44"
        android:layout_below="@+id/textView44"
        android:layout_marginTop="19dp" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView45"
        android:layout_alignLeft="@+id/textView45"
        android:layout_alignRight="@+id/textView45"
        android:layout_alignStart="@+id/textView45"
        android:layout_below="@+id/textView45"
        android:layout_marginTop="19dp" />

</android.widget.RelativeLayout>
